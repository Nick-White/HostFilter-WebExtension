{
	"manifest_version": 2,
	"name": "HostFilter-WebExtension",
	"version": "0.1",

	"options_ui": {
		"page": "options/options.html"
	},
	
	"permissions": [
		"<all_urls>",
		"storage",
		"tabs",
		"webNavigation",
		"webRequest",
		"webRequestBlocking"
	],
	
	"background": {
		"scripts": ["main.js"]
	},
	
	"web_accessible_resources": [
		"empty_content/text.txt"
	]
}